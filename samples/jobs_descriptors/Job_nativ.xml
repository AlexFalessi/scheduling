			<!-- Task human description -->
			<description>Will display 10 dots every 1s</description>
			<scriptExecutable>
				<script>
					<code language="groovy">
						println "hello bob"
					</code>
				</script>
			</scriptExecutable>
		</task>
	</taskFlow>
</job>

