#!/usr/bin/env bash

CLASSPATH=.
workingDir=`dirname $0`
. $workingDir/env rm-log4j-server

opt="-Xms128m -Xmx2048m"
JAVACMD+=(-Dpa.rm.db.hibernate.dropdb=true -Dderby.stream.error.file="$PA_SCHEDULER/.logs/derby.log"  -Dproactive.pamr.agent.id=0 -Dproactive.pamr.agent.magic_cookie=rm $opt org.ow2.proactive.resourcemanager.utils.RMStarter "$@")
exec "${JAVACMD[@]}"

