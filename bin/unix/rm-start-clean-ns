#!/usr/bin/env bash

# Start rm without node sources but with all history 

CLASSPATH=.
workingDir=`dirname $0`
. $workingDir/env rm-log4j-server

opt="-Xms128m -Xmx2048m"
JAVACMD+=(-Dpa.rm.db.hibernate.dropdb.nodesources=true -Dderby.stream.error.file="$PA_SCHEDULER/logs/derby.log" $opt org.ow2.proactive.resourcemanager.utils.RMStarter "$@")
exec "${JAVACMD[@]}"

